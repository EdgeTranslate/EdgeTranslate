{
    "applications": {
        "gecko": {
            "id": "nickyfeng@edgetranslate.com",
            "strict_min_version": "60.0"
        }
    },
    "permissions": [
        "notifications",
        "contextMenus",
        "storage",
        "tabs",
        "<all_urls>",
        "webRequest",
        "webRequestBlocking"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./content/display/display.js"],
            "css": ["./content/display/style/display.css"]
        },
        {
            "matches": ["<all_urls>"],
            "run_at": "document_start",
            "js": ["./content/select/select.js"],
            "css": ["./content/select/select.css"],
            "all_frames": true
        }
    ]
}
